{
  "name": "@argonjs/aframe-extensions",
  "version": "1.1.1",
  "description": "A-Frame integration with Argon.",
  "main": "src/index.js",
  "homepage": "https://aframe.argonjs.io/",
  "scripts": {
    "build": "browserify examples/main.js -o examples/build.js",
    "build-dev": "npm run build && node ./build-site.js --config=dev",
    "build-dist": "npm run build && node ./build-site.js --config=dist",
    "devsite": "npm run build && jekyll build --config _config-develop.yml",
    "testsite": "npm run build && jekyll build --config _config-test.yml",
    "dev": "budo _site/main.js:build.js --dir _site --port 8001 --live --open",
    "devssl": "budo _site/main.js:build.js --dir _site --port 8001 --live --open --ssl --cert .ssl/bmaci-com.crt.pem --key .ssl/bmaci-com.key.pem",
    "dist": "webpack src/index.js dist/argon-aframe.js && webpack -p src/index.js dist/argon-aframe.min.js",
    "distsite": "jekyll build --config _config-master.yml",
    "postpublish": "npm run dist",
    "test": "karma start ./tests/karma.conf.js",
    "unboil": "node scripts/unboil.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argonjs/argon-aframe.git"
  },
  "keywords": [
    "aframe",
    "argon",
    "argonjs",
    "argon-aframe",
    "argon-aframe-component",
    "argon-aframe-components",
    "aframe-argon-component",
    "aframe-argon-components",
    "aframe-argon",
    "layout",
    "aframe-ar",
    "ar"
  ],
  "author": "Blair MacIntyre <blair@cc.gatech.e>",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/argonjs/argon-aframe/issues"
  },
  "dependencies": {
    "aframe": "^0.5.0",
    "@argonjs/argon": "^1.4.0-0"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "budo": "^9.2.1",
    "chai": "^3.4.1",
    "chai-shallow-deep-equal": "^1.3.0",
    "commander": "^2.11.0",
    "ghpages": "0.0.3",
    "glob": "^7.1.2",
    "json-loader": "^0.5.4",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-chai-shallow-deep-equal": "0.0.4",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-sinon-chai": "^1.1.0",
    "mocha": "^2.3.4",
    "mustache": "^2.3.0",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.1",
    "webpack": "^1.12.9"
  }
}
